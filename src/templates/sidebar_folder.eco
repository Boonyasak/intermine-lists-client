<% if @path isnt '/': %>
    <a class="folder <%= @cid %> toggle" title="Toggle folder"></a>
    <a class="folder <%= @cid %> select" title="Select folder">
        <span class="icon-folder-close"></span> <%= @name %>
    </a>
<% end %>
<ul class="nav nav-list lists"></ul>
<ul class="nav nav-list folders"></ul>